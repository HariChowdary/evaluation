<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:layout_marginBottom="5dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:background="@color/white"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_centerHorizontal="true"
        android:textSize="22sp"
        android:id="@+id/sumry"
        android:textStyle="bold"
        android:visibility="gone"
        android:layout_marginTop="5dp"
        android:text="Summary"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:id="@+id/top"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:weightSum="2"
        android:layout_marginTop="7dp"
        android:layout_below="@+id/sumry"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/title_id"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
           android:text="Evaluator ID"
            android:gravity="center"
            android:textStyle="bold"
            android:textColor="#000000"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Subject Code"
            android:gravity="center"
            android:textStyle="bold"
            android:textColor="#000000"
            />
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Bundle Number"
            android:gravity="center"
            android:visibility="gone"
            android:textStyle="bold"
            android:textColor="#000000"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/midl"
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:weightSum="2"
        android:layout_below="@+id/top"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#000000"
            android:id="@+id/evaluatorid"/>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#000000"
            android:id="@+id/sub_id"/>
        <TextView
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:visibility="gone"
            android:textColor="#000000"
            android:id="@+id/bundle_id"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:id="@+id/btm"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/back"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:background="@drawable/selector_button"
            android:text="Back"
            android:visibility="gone"
            android:layout_marginRight="50dp"
            android:textColor="@color/white"
            android:textSize="20sp" />
    <Button
        android:id="@+id/btn_ok"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:background="@drawable/selector_button"
        android:text="Submit"
        android:textColor="@color/white"
        android:textSize="20sp" />
    </LinearLayout>
    <ScrollView
        android:layout_below="@+id/midl"
        android:layout_above="@+id/btm"
        android:layout_width="match_parent"
        android:layout_margin="7dp"
        android:layout_height="match_parent">
